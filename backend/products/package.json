    "@types/jest": "26.0.20",
    "@types/google-protobuf": "^3.7.4",
    "@types/node": "^14.14.31",
    "@types/express": "4.17.11",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "dtslint": "^4.0.7",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
