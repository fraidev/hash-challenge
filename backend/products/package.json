    "dtslint": "^4.0.7",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-sonarjs": "^0.6.0",
    "grpc-tools": "^1.10.0",
    "grpc_tools_node_protoc_ts": "^5.1.3",
    "protobufjs": "^6.10.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "typescript": "^4.1.5"
  }
